{
  "name": "fhir-crud-client",
  "version": "2.0.0",
  "description": "A simple REST client for CRUD operations on FHIR server",
  "repository": "https://github.com/mgramigna/fhir-crud-client",
  "main": "build/index.js",
  "types:": "build/index.d.ts",
  "files": [
    "build/*"
  ],
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "build:watch": "rm -rf ./build && tsc -w",
    "lint": "tsc && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --check \"**/*.{js,ts}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "npm run build"
  },
  "author": "mgramigna",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.21.1",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.19.1",
    "nock": "^11.7.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
